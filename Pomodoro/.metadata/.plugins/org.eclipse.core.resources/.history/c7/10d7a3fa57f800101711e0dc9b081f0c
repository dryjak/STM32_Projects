#include "SSD1306_OLED.h"
#include "main.h"

I2C_HandleTypeDef *SSD1306_i2c;

void SSD1306_Command(uint8_t Command)
{
	HAL_I2C_Mem_Write(SSD1306_i2c, SSD1306_de, MemAddress, MemAddSize, pData, Size, Timeout), DevAddress, MemAddress, MemAddSize, pData, Size, Timeout)
}

void SSD1306_Init(I2C_HandleTypeDef *i2c)
{
	SSD1306_i2c = i2c;

}




